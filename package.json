{
  "name": "fivethree",
  "version": "0.5.0",
  "author": "Fivethree Team",
  "homepage": "http://fivethree.io/",
  "description": "Fivethree Web Components",
  "keywords": [
    "fivethree",
    "angular",
    "ionic",
    "framework",
    "components",
    "ux",
    "mobile",
    "app",
    "webapp",
    "pwa"
  ],
  "scripts": {
    "build": "ng build core && ng build google-maps",
    "build:prod": "ng build core --prod && ng build google-maps --prod",
    "gh-pages": "ng build core && ng build --base-href 'https://fivethree-team.github.io/fivethree/' && npx ngh --dir=www",
    "publish": "npm publish dist/@fivethree/core",
    "rocket": "npm run build:prod && npm run publish",
    "version": "conventional-changelog -p angular -i ./CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "7.2.2",
